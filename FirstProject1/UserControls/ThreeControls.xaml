<UserControl x:Class="FirstProject1.UserControls.ThreeControls"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FirstProject1.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="300"
             x:Name="root">
    <Grid>
        <Button Content="{Binding MyCommand,ElementName=root}" HorizontalAlignment="Left" Margin="33,75,0,0" VerticalAlignment="Top"/>
        <Label Content="{Binding MyText,ElementName=root}" HorizontalAlignment="Left" Margin="33,26,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
